{
	"AWSEBDockerrunVersion": 2,
	"containerDefinitions": [
		{
			"name": "api",
			"image": "isometria/dkw-10-api",
			"hostname": "api",
			"essential": false
		},
		{
			"name": "client",
			"image": "isometria/dkw-10-client",
			"hostname": "client",
			"essential": false
		},
		{
			"name": "nginx",
			"image": "isometria/dkw-10-nginx",
			"hostname": "nginx",
			"essential": true,
			"portMappings": [
				{
					"hostPort": 80,
					"containerPort": 80
				}
			],
			"links": [ "api", "client" ]
		},
		{
			"name": "worker",
			"image": "isometria/dkw-10-worker",
			"hostname": "worker",
			"essential": false
		}
	]
}